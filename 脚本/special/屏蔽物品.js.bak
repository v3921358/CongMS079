var useNx = 400;
var sel0 = -1;
function start() {
    status = -1;
    action(1, 0, 0);
}

function action(mode, type, selection) {
    if (status == 0 && mode == 0) {
        cm.dispose();
        return;
    }
    if (mode == 1) {
        status++;
    } else {
        status--;
    }

    if (status == 0) {
    	var txt = "#d\t\t\t#b欢迎来到『冒险岛』抽奖中心#n#k\r\n";
    	txt += "#r#L1##b屏蔽物品#l\r\n\r\n\r\n";
		txt += "#d#L2##b删除屏蔽#l\r\n\r\n\r\n";
		txt += "\t#b奖池 \r\n";
		
		var txt2 = "";
		for (var i = 0; i < itemList1.length;  i++){
			txt2 += "#i"+itemList1[i][0]+":#";
		}
    	cm.sendSimple(txt + txt2);
    } else if (status == 1) {
		sel0 = selection;
		cm.sendGetNumber("#d请输入屏蔽/解除屏蔽的物品ID\r\n"
		//+"#d枫叶抽奖400枫叶/400点卷一次\r\n"
		//+"#r抽奖有保底，请看群文件"
		+"#r当前拥有枫叶数量: #v4001126##r#c4001126#个#k\r\n#r " ,
		1, 1, 9999999
		);
	} else if(status == 2) {
		/*
		if (!cm.checkNumSpace(0, selection)) {
			cm.sendOk("背包空间不足"+selection+"格");
			cm.dispose();
			return;
		}
		*/
		if(selection > 9999999 && selection < 1000000){
			cm.sendOk("请输入正确的ID");
			cm.dispose();
		}
    	switch (sel0) {
			case 1:
				if(cm.getBossRankCount("屏蔽"+selection) >= 1){
					cm.sendOk("你已屏蔽过物品ID"+selection+",请勿多次屏蔽！");
					break;
				}else{
					cm.setBossRankCount("屏蔽"+selection, 1);
					cm.sendOk("你已屏蔽物品ID"+selection+"");
					break;
				}
				break;
			case 2:
				if(cm.getBossRankCount("屏蔽"+selection) > 0){
					cm.setBossRankCount("屏蔽"+selection, -1);
					cm.sendOk("你已取消屏蔽物品ID"+selection+"");
					break;
				}else{
					cm.sendOk("你还未屏蔽过物品ID"+selection+"");
					break;
				}
				break;
			default:
				cm.sendOk("脚本出错，请联系管理员");
				cm.dispose();
				return;
        }
		cm.dispose();
		return;
    }
}